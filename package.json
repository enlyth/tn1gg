{
  "name": "tn1gg",
  "private": false,
  "devDependencies": {
    "lerna": "^5.1.6",
    "nx": "^14.3.6"
  },
  "dependencies": {
    "concurrently": "^7.2.2"
  },
  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -c \"black.bgMagenta,black.bgCyan\" --kill-all --handle-input \"yarn:dev-api\" \"yarn:dev-web\"",
    "dev-web": "cd packages/tn1gg-web && yarn dev",
    "dev-api": "cd packages/tn1gg-api && yarn dev"
  }
}